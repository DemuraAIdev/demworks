{
  "name": "demworks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "concurrently \"next dev --turbo --show-all\" \"tailwindcss --input styles/globals.css --output styles/globals.css --watch\"",
    "build:turbo": "tailwindcss styles/globals.css --output output.css && next build",
    "build": "ANALYZE=true next build",
    "launch": "next build && next-sitemap",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/bundle-analyzer": "^13.0.2",
    "@next/mdx": "^13.0.1",
    "@tailwindcss/typography": "^0.5.7",
    "@types/mdx": "^2.0.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "eslint": "8.57.0",
    "eslint-config-next": "13.0.1",
    "next": "13.0.1",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^3.1.30",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1"
  }
}